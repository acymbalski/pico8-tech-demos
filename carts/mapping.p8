__lua__

-- called by program to initialize variables
function _init()
    -- draw only once!

    -- clear screen
    cls()

    -- draw sky (blue rect)
    rectfill(0, 0, 79, 23, 12)

    -- draw random clouds
    for i=0, flr(rnd(2)) + 1 do
        map(0, 0, flr(rnd(16)) + i * 16, flr(rnd(16)) - 8, 2, 2)
    end

    -- draw hills
    for i=0, 4 do
        map(2, 0, i * 16, 8, 2, 2)
    end
end

-- called by program to update variables
